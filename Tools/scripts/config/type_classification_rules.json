{
  "classification_rules": {
    "rules": [
      {
        "key": "defaultValue",
        "value": null,
        "type": "action"
      },
      {
        "key": "defaultValue",
        "value": ["true", "false"],
        "type": "bool"
      },
      {
        "key": "description",
        "contains": "bitmask",
        "type": "bitmask"
      },
      {
        "key": "defaultValue",
        "is_numeric": true,
        "contains_decimal": true,
        "type": "float"
      },
      {
        "key": "defaultValue",
        "is_numeric": true,
        "contains_decimal": false,
        "type": "unknown_numeric"
      }
    ],
    "default_type": "string"
  },
  "numeric_detection_rules": {
    "min_occurrences": 4,
    "float_ratio": 0.5,
    "min_int_occurrences": 15,
    "unknown_types": ["unknown", "unknown_numeric"],
    "protected_types": ["float", "bool", "bitmask", "action"]
  },
  "popularity_rules": {
    "popularity_threshold": 0.10
  }
}
